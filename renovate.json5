{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: ["config:base", "github>aquaproj/aqua-renovate-config#1.8.0"],

  ignorePresets: [":prHourlyLimit2"],
  branchConcurrentLimit: 0,
  prHourlyLimit: 0,
  automerge: true,
  platformAutomerge: true,

  kubernetes: {
    fileMatch: ["^k8s/app/.+\\.yaml$", "^kind/.+\\.yaml$"],
  },

  labels: ["dependencies"],
  packageRules: [
    {
      matchPackagePatterns: ["golangci-lint"],
      labels: ["golangci-lint"],
    },
  ],

  hostRules: [
    {
      hostType: "docker",
      username: "kimitsu",
      encrypted: {
        password: "",
      },
    },
  ],

  reviewers: ["YunosukeY"],
}
